{
	"publisher": "christianbrassat",
	"name": "contentq",
	"displayName": "contentQ",
	"description": "InsertContent made easy",
	"version": "0.0.3",
	"repository": {
		"type": "git",
		"url": "https://github.com/crshd/vscode-contentQ.git"
	},
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.insertcontent"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.insertcontent",
				"title": "contentQ: InsertContent"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}